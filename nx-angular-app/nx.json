{
  "npmScope": "smec-monorepo",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "adminapp": {
      "tags": ["scope:admin"]
    },
    "adminapp-e2e": {
      "tags": ["scope:admin"],
      "implicitDependencies": ["adminapp"]
    },
    "core-app": {
      "tags": ["scope:shared"]
    },
    "fuse": {
      "tags": ["scope:shared"]
    },
    "layout": {
      "tags": ["scope:shared"]
    },
    "modules-claims-data-access": {
      "tags": ["scope:admin"]
    },
    "modules-claims-feature-claims": {
      "tags": ["scope:admin"]
    },
    "modules-claims-feature-claims-edit": {
      "tags": ["scope:admin"]
    },
    "modules-claims-shell-web": {
      "tags": ["scope:admin"]
    },
    "modules-profile-feature-profile-edit": {
      "tags": ["scope:shared"]
    },
    "modules-profile-shell-mobile": {
      "tags": ["scope:shared"]
    },
    "modules-profile-shell-web": {
      "tags": ["scope:shared"]
    },
    "modules-roles-data-acess": {
      "tags": ["scope:admin"]
    },
    "modules-roles-feature-permissions": {
      "tags": ["scope:admin"]
    },
    "modules-roles-feature-roles": {
      "tags": ["scope:admin"]
    },
    "modules-roles-feature-roles-edit": {
      "tags": ["scope:admin"]
    },
    "modules-roles-shell-web": {
      "tags": ["scope:admin"]
    },
    "modules-users-data-access": {
      "tags": ["scope:admin"]
    },
    "modules-users-feature-users": {
      "tags": ["scope:admin"]
    },
    "modules-users-feature-users-add": {
      "tags": ["scope:admin"]
    },
    "modules-users-feature-users-edit": {
      "tags": ["scope:admin"]
    },
    "modules-users-shell-mobile": {
      "tags": ["scope:admin"]
    },
    "modules-users-shell-web": {
      "tags": ["scope:admin"]
    },
    "shared-angular-material-components": {
      "tags": ["scope:shared"]
    },
    "shared-confirm-dialog": {
      "tags": ["scope:shared"]
    },
    "shared-data-access": {
      "tags": ["scope:shared"]
    },
    "shared-environments": {
      "tags": ["scope:shared"]
    },
    "shared-guards": {
      "tags": ["scope:shared"]
    },
    "shared-login": {
      "tags": ["scope:shared"]
    },
    "shared-models": {
      "tags": ["scope:shared"]
    },
    "shared-ui": {
      "tags": ["scope:shared"]
    },
    "shared-utils": {
      "tags": ["scope:shared"]
    },
    "smecapp": {
      "tags": ["scope:client"]
    },
    "smecapp-e2e": {
      "tags": ["scope:client"],
      "implicitDependencies": ["smecapp"]
    }
  }
}
