{
  "npmScope": "smec-monorepo",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "adminapp": {
      "tags": [
        "scope:admin"
      ]
    },
    "adminapp-e2e": {
      "tags": [
        "scope:admin"
      ],
      "implicitDependencies": [
        "adminapp"
      ]
    },
    "shared-data-access": {
      "tags": [
        "scope:shared"
      ]
    },
    "shared-environments": {
      "tags": [
        "scope:shared"
      ]
    },
    "shared-login": {
      "tags": []
    },
    "shared-models": {
      "tags": [
        "scope:shared"
      ]
    },
    "shared-ui": {
      "tags": [
        "scope:shared"
      ]
    },
    "shared-utils": {
      "tags": []
    },
    "smecapp": {
      "tags": [
        "scope:client"
      ]
    },
    "smecapp-e2e": {
      "tags": [
        "scope:client"
      ],
      "implicitDependencies": [
        "smecapp"
      ]
    },
    "users-feature-user-detail": {
      "tags": [
        "scope:client"
      ]
    },
    "users-feature-users-list": {
      "tags": [
        "scope:client"
      ]
    },
    "users-shell-mobile": {
      "tags": [
        "scope:client"
      ]
    },
    "users-shell-web": {
      "tags": [
        "scope:client"
      ]
    }
  }
}
