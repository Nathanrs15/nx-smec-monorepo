{
  "npmScope": "smec-monorepo",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "projects": {
    "adminapp": {
      "tags": ["scope:admin"]
    },
    "adminapp-e2e": {
      "tags": ["scope:admin"],
      "implicitDependencies": ["adminapp"]
    },
    "shared-ui": {
      "tags": ["scope:shared"]
    },
    "smecapp": {
      "tags": ["scope:client"]
    },
    "smecapp-e2e": {
      "tags": ["scope:client"],
      "implicitDependencies": ["smecapp"]
    },
    "smecapp-feature-item": {
      "tags": ["scope:client"]
    }
  }
}
