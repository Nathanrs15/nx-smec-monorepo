          }
        }
      }
    },
    "smecapp-feature-item": {
      "projectType": "library",
      "root": "libs/smecapp/feature-item",
      "sourceRoot": "libs/smecapp/feature-item/src",
      "prefix": "smec-monorepo",
      "architect": {
